package pageObjects;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;

public class HomeStatementAboutYou {
	
	public WebDriver driver;
	
	private By page2No = By.xpath("//button[contains(text(),'Next')]");
	private By page2Next = By.xpath("//button[contains(text(),'Next')]");
	
	
	public HomeStatementAboutYou(WebDriver driver) {
		// TODO Auto-generated constructor stub
		
		this.driver=driver;
		
	}
	
	
	
	
	
	
	public WebElement firstName()
	{
		return driver.findElement(page2No);
	}
	
	public WebElement firstName()
	{
		return driver.findElement(page2No);
	}

}
