package pageObjects;



import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;

public class LandingPage7 {
	
	public WebDriver driver;
	
	private By insureYears = By.xpath("//button[@id='downshift-635-toggle-button']");
	private By claim  = By.xpath("//button[contains(text(),'No')]");
	private By next7  = By.xpath("//button[@class='styled__PrimaryButton-sc-qy8nbl-2 Sectionstyle__PrimaryButton-sc-b4fcid-4 lcLfTk fPpkjj']");
	
	
	
	
	
	
	public LandingPage7(WebDriver driver) {
		// TODO Auto-generated constructor stub
		
		this.driver=driver;
		
	}
	
	

	
	public WebElement getinsureYears()
	{
		return driver.findElement(insureYears);
	}
	
	
	public WebElement getclaim()
	{
		return driver.findElement(claim);
	}
	
	
	public WebElement getnext7()
	{
		return driver.findElement(next7);
	}

	


}
